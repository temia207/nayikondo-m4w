              <mapsTo>repairDetails</mapsTo>
            </mapping>
            <mapping>
              <expression query="&lt;problemFixed&gt;{/WaterFlow/waterPointIsFixed/text()}&lt;/problemFixed&gt;" />
              <mapsTo>problemFixed</mapsTo>
            </mapping>
            <mapping>
              <expression query="&lt;action&gt;assessment&lt;/action&gt;" />
