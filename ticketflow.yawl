        <task id="Notify_HPM_3">
          <name>Notify HPM</name>
          <flowsInto>
            <nextElementRef id="Notify_Sender_4" />
          </flowsInto>
          <join code="xor" />
          <split code="and" />
          <startingMappings>
            <mapping>
              <expression query="&lt;message&gt;Water point problem: {/WaterFlow/village/text()}, + {/WaterFlow/waterPointName/text()} : + {/WaterFlow/msgTicketCommentFromSender/text()}&lt;/message&gt;" />
